{
	"$schema": "https://biomejs.dev/schemas/1.7.0/schema.json",
	"files": {
		"ignore": ["./src/**/*.gen.ts"]
	},
	"organizeImports": {
		"enabled": false
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"javascript": {
		"formatter": {
			"semicolons": "asNeeded"
		}
	}
}
